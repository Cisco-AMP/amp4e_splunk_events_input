{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.1",
    "@splunk/splunk-utils": "^2.2.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.5",
    "react-scripts": "5.0.1",
    "react-select": "^5.7.0",
    "styled-components": "^5.3.6"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build && mv build/static/js/main*.js build/static/js/main.min.js",
    "watch": "watch 'npm run build' src"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "rules": {
          "semi": [
            "error",
            "never"
          ]
        },
        "files": [
          "*.js",
          "*.jsx"
        ]
      }
    ]
  },
  "prettier": {
    "semi": false,
    "trailingComma": "none"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "watch": "^1.0.2"
  }
}
