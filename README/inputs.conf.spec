[amp4e_events_input://<name>]
    * Set up an input to manage AMP event stream

stream_name = <value>
    * Event stream name

event_types = <list>
    * Event types for the stream

groups = <list>
    * Group guids for the stream

api_host = <value>
    * AMP API host

api_id = <value>
    * 3rd Party API Client ID provided by AMP

api_key = <value>
    * The secret API key
